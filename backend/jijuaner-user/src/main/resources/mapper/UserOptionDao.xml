<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yunzen.jijuaner.user.dao.UserOptionDao">
    <select id="selectGroupByUserIdAndGroupId" resultType="com.yunzen.jijuaner.user.entity.UserOptionEntity">
        SELECT *
        FROM option_group_fund
        WHERE user_id = #{userId} and group_id = #{groupId}
    </select>

</mapper>
